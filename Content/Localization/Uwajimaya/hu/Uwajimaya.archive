					},
					"Translation":
					{
						"Text": "Fura Eszköz"
					},
					"Key": "SecretWeirdDevice_name"
				},
				{
					"Source":
					{
						"Text": "Weird Key"
					},
					"Translation":
					{
						"Text": "Fura Kulcs"
					},
					"Key": "KeyAAFirstFight_name"
				},
